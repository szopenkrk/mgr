{% extends "MyWorkBundle::layout.html.twig" %}

{% block body %}

    {% block container %}
        Lista dostępnych konferencji
        <ul>
            {%  for entitie in entities %}
                <li>
                    {{ entitie.titleConference }}
                    {{ entitie.placeConference }}
                    {{ entitie.dateConference|date('Y-m-d') }}
                    <a href="#" accesskey="">Weź udział w konferencji</a>
                    <a href="#">Referuj na konferencji</a>
                </li>
            {% endfor %}
        </ul>
    {% endblock %}

{% endblock %}
